.docs_paperpd{position:relative;z-index:0;width:798px;margin:30px auto 0;padding:1px 0;zoom:1}
.docs_paperpd:first-child{margin-top:-1px}
.docs_paperpd .docs_bor{display:block;position:absolute;left:0;top:0;right:0;bottom:0;margin:0 -1px;border:1px solid #000;opacity:0.6;filter:alpha(opacity=60)}
.docs_paper{position:relative;z-index:10;height:1011px;padding:57px 60px;background:#fff;font-size:12px;line-height:16px;color:#434343;zoom:1}
.docs_paper h2{position:relative;z-index:10;font-size:24px;line-height:28px}
.docs_paper p{position:relative;z-index:10;min-height:16px;margin-top:4px}
.docs_paper .txt{margin-right:1px;padding:1px 0}
.docs_paper .txt.bgcolor1{background:#ff6351}
.docs_paper .txt.bgcolor2{background:#ff8a36}
.docs_paper .txt.bgcolor3{background:#f8db4d}
.docs_paper .txt.bgcolor4{background:#98e64c}
.docs_paper .txt.bgcolor5{background:#4aeeb6}
.docs_paper .txt.bgcolor6{background:#4dc2ff}
.docs_paper .txt.bgcolor7{background:#4f88ff}
.docs_paper .txt.bgcolor8{background:#8f84ff}
.docs_paper .txt.bgcolor9{background:#bf7dff}
.docs_paper .txt.bgcolor10{background:#ff69c4}
.docs_paper .txt.color1{color:#ff6351}
.docs_paper .txt.color2{color:#ff8a36}
.docs_paper .txt.color3{color:#f8db4d}
.docs_paper .txt.color4{color:#98e64c}
.docs_paper .txt.color5{color:#4aeeb6}
.docs_paper .txt.color6{color:#4dc2ff}
.docs_paper .txt.color7{color:#4f88ff}
.docs_paper .txt.color8{color:#8f84ff}
.docs_paper .txt.color9{color:#bf7dff}
.docs_paper .txt.color10{color:#ff69c4}
.docs_paper .edit_num{display:inline-block;visibility:hidden;position:absolute;left:-46px;width:29px;font-family:tahoma;font-size:10px;font-weight:normal;color:#a0a0a0;text-align:center;vertical-align:top}
.docs_paper .start_color{display:inline-block;position:absolute;left:100%;top:4px;width:120px;margin-left:60px;color:#a0a0a0;text-align:center;vertical-align:top;white-space:nowrap}
.docs_paper .start_color .pin{display:inline-block;float:left;position:relative;margin-left:10px;font-size:11px;line-height:15px;color:#fff;vertical-align:top;white-space:nowrap;cursor:default}
.docs_paper .start_color .circle{display:inline;float:left;width:8px;height:8px}
.docs_paper .start_color .color1 .circle{background-position:0 0}
.docs_paper .start_color .color2 .circle{background-position:-10px 0}
.docs_paper .start_color .color3 .circle{background-position:-20px 0}
.docs_paper .start_color .color4 .circle{background-position:-30px 0}
.docs_paper .start_color .color5 .circle{background-position:-40px 0}
.docs_paper .start_color .color6 .circle{background-position:-50px 0}
.docs_paper .start_color .color7 .circle{background-position:-60px 0}
.docs_paper .start_color .color8 .circle{background-position:-70px 0}
.docs_paper .start_color .color9 .circle{background-position:-80px 0}
.docs_paper .start_color .color10 .circle{background-position:-90px 0}
.docs_paper .start_color .name{display:inline-block;float:left;margin:-3px 0 0 4px;vertical-align:top}
.docs_paper .start_color .name strong{display:inline-block;vertical-align:top}
.docs_paper .start_color .name strong .line{display:none}
.docs_paper .start_color .name .arrow{display:none}

.docs_rwrap{position:absolute;right:0;top:39px;bottom:0;z-index:20;width:162px;background:#fff}
.docs_rwrap .docs_rbor{display:block;visibility:hidden;overflow:hidden;position:absolute;top:-1px;left:0;right:0;height:1px;background:#000;font-size:0;line-height:0;opacity:0.6;filter:alpha(opacity=60)}
.docs_rcont{position:absolute;right:0;top:0;bottom:0;left:0}
.docs_rcont .bg_line{display:block;overflow:hidden;position:absolute;left:-1px;top:0;bottom:0;width:1px;background:#000;opacity:0.3;filter:alpha(opacity=30)}
.docsr_over{overflow:auto;position:absolute;left:0;top:0;right:0;bottom:0;width:100%;margin-left:-6px;padding-left:6px/*;padding-right:12px*/}
.docsr_zoom{width:auto;zoom:1}
.docsr_list{width:100%}
.docsr_list li{width:100%;vertical-align:top}
.docsr_list li .box{display:block;position:relative;margin-top:-1px;padding:11px 12px 10px;line-height:14px;zoom:1}
.docsr_list li .ver{font-size:11px;font-weight:bold;color:#666}
.docsr_list li .bar{position:absolute;right:100%;top:50%;width:9px;height:9px;margin:-5px -4px 0 0;background-position:9px 9px}
.docsr_list li .color1 .ver{color:#ff6351}
.docsr_list li .color2 .ver{color:#ff8a36}
.docsr_list li .color3 .ver{color:#f8db4d}
.docsr_list li .color4 .ver{color:#98e64c}
.docsr_list li .color5 .ver{color:#00a3ee}
.docsr_list li .color6 .ver{color:#4dc2ff}
.docsr_list li .color7 .ver{color:#4f88ff}
.docsr_list li .color8 .ver{color:#8f84ff}
.docsr_list li .color9 .ver{color:#bf7dff}
.docsr_list li .color10 .ver{color:#ff69c4}
.docsr_list li .color1 .bar{background-position:-180px -215px}
.docsr_list li .color2 .bar{background-position:-170px -215px}
.docsr_list li .color3 .bar{background-position:-160px -215px}
.docsr_list li .color4 .bar{background-position:-150px -215px}
.docsr_list li .color5 .bar{background-position:-140px -215px}
.docsr_list li .color6 .bar{background-position:-130px -215px}
.docsr_list li .color7 .bar{background-position:-120px -215px}
.docsr_list li .color8 .bar{background-position:-110px -215px}
.docsr_list li .time{margin-top:2px;font-size:11px;font-family:tahoma;color:#a0a0a0}
.docsr_list li .line{display:block;overflow:hidden;position:absolute;left:12px;bottom:0;right:12px;z-index:30;height:1px;background:#e5e5e5}
.docsr_list li a:hover,.docsr_list li a:focus{z-index:10;padding-top:10px;padding-bottom:9px;border:1px solid #e5e5e5;border-left:0 none;border-right:0 none;background:#fafafa}
.docsr_list li:first-child a:hover,.docsr_list li:first-child a:focus{border-top-color:#cfd0d0}
.docsr_list li a:hover .line,.docsr_list li a:focus .line{display:none}

.bar_scroll{display:none;position:absolute;right:0;top:0;bottom:0;width:11px;border-left:1px solid #e5e5e5}
.bar_pd{position:absolute;left:2px;width:7px;padding:3px 0}
.bar_pd .bar{height:100%;background:#b7b7b7}
.bar_pd .spdocs{position:absolute;left:0;width:7px;height:3px}
.bar_pd .bar_bgt{top:0;background-position:-110px -228px}
.bar_pd .bar_bgb{bottom:0;background-position:-110px -231px}

.docs_noti{position:absolute;z-index:10;top:0;left:0;right:0;height:47px;border-bottom:1px solid #cac9c0;background:#fffef0;font-size:11px;color:#666;text-align:center}
.docs_noti.docs_rview{right:162px}
.docs_noti p{padding-top:16px;line-height:16px}
.docs_noti p.row2{padding-top:8px}
.docs_noti .clse{display:inline-block;position:absolute;right:0;top:0;padding:16px;font-size:0;line-height:0;vertical-align:top}
.docs_noti .clse span{display:inline-block;overflow:hidden;width:15px;height:15px;background:url(../images/sp_group.png) no-repeat -100px -120px;line-height:999px;vertical-align:top}

/* 풀스크린일 경우 */
.fullscreen .docs_viewpd{z-index:10}
.fullscreen .docs_viewpd .docs_viewbor{visibility:visible;left:0;right:0;width:auto;margin:0}
.fullscreen .docs_view{background:#fff}
.fullscreen .docs_paperwrap{padding-bottom:0}
.fullscreen .docs_paperpd{width:auto;margin-top:-1px;padding-bottom:0}
.fullscreen .docs_bor{visibility:hidden;margin:0;border-left:0 none;border-right:0 none}
.fullscreen .docs_paper{height:auto;padding:43px 46px;background:transparent}
.fullscreen .docs_paper .start_color{left:auto;right:-26px;width:auto;margin:0;color:#a0a0a0}
.fullscreen .docs_paper .start_color .pin{margin-left:2px;color:#666;cursor:pointer}
.fullscreen .docs_paper .start_color .circle{display:inline-block;float:none}
.fullscreen .docs_paper .start_color .name{display:inline-block;visibility:hidden;float:none;position:absolute;bottom:100%;left:50%;width:48px;margin:0 0 7px -24px;text-align:center}
.fullscreen .docs_paper .start_color .name strong{position:relative;padding:0 2px;border:1px solid #666;background:#fff}
.fullscreen .docs_paper .start_color .name strong .line{display:block;overflow:hidden;position:absolute;left:-1px;top:100%;right:-1px;height:1px;margin-top:1px;background:#000;opacity:0.1;filter:alpha(opacity=10)}
.fullscreen .docs_paper .start_color .name .arrow{display:inline-block;position:absolute;left:50%;top:100%;width:8px;height:6px;margin:-1px 0 0 -4px;background-position:0 -10px}
.fullscreen .docs_paper .start_color .pin:hover .name,.fullscreen .docs_paper .start_color .pin:focus .name{visibility:visible}
.fullscreen .docs_paper .edit_num,
.fullscreen .docs_view .docs_line{visibility:visible}
.fullscreen .docs_rwrap{top:88px}
.fullscreen .docs_rwrap .docs_rbor{visibility:visible}
.fullscreen .docs_rview .docs_history{right:-162px}
