@charset "utf-8";
/* Hivelab 130118 */
/* Common */
html,body{width:100%;height:100%}
html{overflow:hidden;overflow-x:auto}
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:'나눔고딕',Nanumgothic,'돋움',dotum,Helvetica,sans-serif;font-size:12px;line-height:18px}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{text-decoration:none}
a:hover,a:active,a:focus{text-decoration:none}
legend,hr,caption{display:none}
button{margin:0;padding:0;border:0;background:none}
.skip,.blind{overflow:hidden;position:absolute;top:0;left:0;width:0;height:0;font-size:0;line-height:0}
.input_chk{position:relative;width:13px;height:13px;margin:-2px 0 2px;padding:0;vertical-align:middle}
.relative{position:relative}

/* 스프라이트 */
.spdocs{display:inline-block;overflow:hidden;width:10px;height:10px;background:url(../images/docs/sp_docs.png) no-repeat;line-height:999px;vertical-align:top}

/* layout */
#wrap{position:relative;width:100%;min-width:1280px;min-height:100%}
#container{position:absolute;top:34px;right:0;left:0;bottom:33px;margin:0}
#header{position:relative;width:100%;height:33px;border-bottom:1px solid #000;background:-ms-linear-gradient(top, #404040 0%, #2f2f2f 100%);
background:-webkit-gradient(linear,0% 0,0% 100%,from(#404040),to(#2f2f2f));background:-moz-linear-gradient(50% 0,#404040,#2f2f2f);background:-o-linear-gradient(top,#404040,#2f2f2f);filter:progid:DXImageTransform.Microsoft.gradient(startColorStr=#404040,endColorStr=#2f2f2f);text-align:center}
#footer{overflow:visible;width:100%;position:absolute;left:0;right:0;bottom:0;padding:0;z-index:20;background:#000}

/* header */
#header h1{display:inline;float:left;margin:9px 0 0 15px}
#header h1 a{width:72px;height:18px;background-position:0 -18px}
#header .title_area{display:inline;float:left;margin:8px 0 0 9px;font-size:12px;line-height:16px;color:#fff}
#header .title_area .bar{display:inline;float:left;overflow:hidden;width:0;height:14px;margin-top:2px;border-left:1px solid #0e0e0e;border-right:1px solid #707070;font-size:0;line-height:0}
#header .title_area .tit_cont{display:inline;float:left;margin-left:10px}
#header .title_area .tit_cont .inp_bor{display:inline-block;height:16px;margin:-1px 0 -2px;padding:2px 5px;background:#fff;line-height:16px;vertical-align:top}
#header .title_area .tit_cont .inp_bor input{width:328px;height:16px;*margin:-1px 0;border:0 none;background:transparent;line-height:16px;vertical-align:top}
#header .title_area .btn_edit{display:inline;float:left;margin:-2px 0 0 5px;padding:5px;font-size:0;line-height:0}
#header .title_area .btn_edit .spdocs{width:11px;height:11px;background-position:-87px -40px}
#header .view_type{position:absolute;top:7px;right:15px}
#header .view_type li{display:inline;float:left;margin-left:7px}
#header .view_type li .spdocs{width:19px;height:19px}
#header .view_type li .view1{background-position:0 -61px}
#header .view_type li.on .view1,#header .view_type li .view1:hover,#header .view_type li .view1:focus{background-position:0 -40px}
#header .view_type li .view2{background-position:-20px -40px}
#header .view_type li.on .view2,#header .view_type li .view2:hover,#header .view_type li .view2:focus{background-position:-20px -61px}

/* container */
.docs_bar{position:relative;z-index:20;height:37px;border-top:1px solid #fff;border-bottom:1px solid #cfd0d0;background:#f5f5f5}
.docs_bar .shadow{display:block;position:absolute;left:0;right:0;top:100%;height:4px;margin-top:1px;border-top:1px solid #313131;background:url(../images/docs/bg_barshadow.png) repeat-x 0 -1px}
